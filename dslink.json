{
  "name": "dslink-java-v2-mailer",
  "version": "1.2.1",
  "description": "E-mail link",
  "main": "bin/dslink-java-v2-mailer",
  "configs": {
    "handler_class": {
      "type": "string",
      "value": "org.iot.dsa.dslink.mailer.MainNode"
    },
    "log": {
      "help": "all, trace, debug, info, warn, error, off",
      "type": "enum",
      "value": "info"
    },
    "msgpack": {
      "help": "Whether or not to use msgpack.  False is useful for debugging.",
      "type": "bool",
      "value": true
    },
    "broker": {
      "type": "url",
      "value": null
    },
    "log-file": {
      "help": "Only set when the link is being run standalone",
      "type": "path",
      "value": null
    },
    "key": {
      "help": "Public/private key pair used by the link.",
      "type": "path",
      "value": null
    },
    "name": {
      "help": "Overrides the name above.",
      "type": "string",
      "value": null
    },
    "nodes": {
      "help": "Path to the node configuration database file.",
      "type": "path",
      "value": null
    },
    "token": {
      "help": "Authentication token for the broker.",
      "type": "string",
      "value": null
    }
  }
}
